
@model IEnumerable<nyikoStudyGroup.Models.StudentModel>

@{
    ViewBag.Title = "ListStudents";
}

<h2>ListStudents</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(Model => Model._studentNumber)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model._name)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model._surname)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model._emailAddress)
        </th>
    </tr>

    @foreach (var item in Model)
     {
        <tr>
            <th>
                @Html.DisplayNameFor(Model => Model._studentNumber)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model._name)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model._surname)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model._emailAddress)
            </th>   
        </tr>
     }

</table>